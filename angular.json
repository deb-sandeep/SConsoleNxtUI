{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app-landing": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/app-landing",
      "sourceRoot": "projects/app-landing/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/app-landing",
            "index": "projects/app-landing/src/index.html",
            "browser": "projects/app-landing/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/app-landing/tsconfig.app.json",
            "assets": [
              "projects/app-landing/src/assets",
              {
                  "glob": "**/*",
                  "input": "./projects/lib-core/assets",
                  "output": "/core-assets/"
              }
            ],
            "styles": [
              "projects/app-landing/src/assets/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app-landing:build:production"
            },
            "development": {
              "buildTarget": "app-landing:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    },
    "lib-core": {
      "projectType": "library",
      "root": "projects/lib-core",
      "sourceRoot": "projects/lib-core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/lib-core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/lib-core/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/lib-core/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  }
}
